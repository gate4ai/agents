{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "npm run test:unit && npm run test:integration",
    "test:watch": "NUXT_TELEMETRY_DISABLED=1 vitest",
    "test:unit": " NUXT_TELEMETRY_DISABLED=1 vitest run --config vitest.unit.config.ts",
    "test:integration": "NUXT_TELEMETRY_DISABLED=1 vitest run --config vitest.integration.config.ts",
    "lint": "eslint . --ext .ts,.js,.vue"
  },
  "dependencies": {
    "@google-cloud/speech": "^7.1.0",
    "@google/generative-ai": "^0.15.0",
    "@nuxt/content": "^3.5.1",
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^1.13.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "^0.11.8",
    "@nuxt/ui": "^3.1.3",
    "@types/pino": "^7.0.4",
    "@types/sqlite3": "^3.1.11",
    "@unhead/vue": "^2.0.10",
    "eslint": "^9.28.0",
    "nuxt": "^3.17.5",
    "openai": "^4.52.7",
    "pino": "^9.7.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "typescript": "^5.8.3",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.19.1",
    "@types/supertest": "^6.0.3",
    "happy-dom": "^17.6.3",
    "msw": "^2.9.0",
    "pino-pretty": "^13.0.0",
    "supertest": "^7.1.1",
    "vitest": "^3.2.2"
  }
}
